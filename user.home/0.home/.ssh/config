# Host github.com
# 	User {changeme}
# 	Hostname github.com
# 	Port 22
# 	IdentityFile ~/.ssh/id_rsa
# 	AddKeysToAgent yes

#Disable SSH Host Key Checking For 192.168.0.0/24
Host 192.168.0.*
   StrictHostKeyChecking no
   UserKnownHostsFile=/dev/null

Host *
  IdentityFile ~/.ssh/id_rsa
  AddKeysToAgent yes
